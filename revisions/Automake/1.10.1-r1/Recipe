# Recipe for version 1.10.1 by Giambattista Bloisi <giamby@infinito.it>, on Tue Jan 22 08:53:18 CET 2008
url="$ftpGnu/automake/automake-1.10.1.tar.gz"
file_size=1212534
file_md5=a0acfd1b167ba55a256f0c1af2983975
compile_version=1.8.0
recipe_type=configure
pre_install() {
   pushd $target/Shared
   ln -s automake-1.10.1 automake
   ln -s aclocal-1.10.1 aclocal
   popd
}
