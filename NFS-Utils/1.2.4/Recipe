compile_version=1.8.2
url="https://downloads.sourceforge.net/nfs/nfs-utils-1.2.4.tar.bz2"
file_size=664358
file_md5=938dc0574f3eb9891a8ed4746f806277
recipe_type=configure

configure_options=(
	"--disable-gss"
    "--disable-ipv6"
	"--sysconfdir=$settings_target"
	"--with-statedir=$goboVariable/lib/nfs"
	"--localstatedir=$goboVariable/lib/nfs"
)

install_variables=(
    sbindir=$target/sbin
)

unmanaged_files=(
    $goboVariable/lib/nfs
)
