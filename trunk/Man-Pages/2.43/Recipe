url="ftp://ftp.kernel.org/pub/linux/docs/manpages/man-pages-2.43.tar.bz2"
recipe_type=makefile
do_build=no
install_variables=(
   "MANDIR=$target/man"
)
pre_link() {
   # we have our own hier man page.
   rm -f $target/man/man7/hier.7
}
