pre_link() {
	GrepReplace -B "root" "$(SuperUserName)" "$settings_target/at-spi2/accessibility.conf"
	GrepReplace -B "root" "$(SuperUserName)" "$target/Resources/Defaults/Settings/at-spi2/accessibility.conf"
}
