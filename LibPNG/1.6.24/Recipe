compile_version=1.8.5
url="https://downloads.sourceforge.net/project/libpng/libpng16/1.6.24/libpng-1.6.24.tar.xz"
file_size=977532
file_md5=ffcdbd549814787fa8010c372e35ff25
recipe_type=configure

pre_link() {
    ln -sf libpng16 $target/include/libpng
    ln -sf libpng.so $target/lib/libpng.so.0
}
