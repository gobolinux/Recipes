compile_version=1.8.2
url="https://kernel.org/pub/linux/utils/kernel/module-init-tools/module-init-tools-3.4.tar.bz2"
file_size=190437
file_md5=db6ac059e80e8dd4389dbe81ee61f3c6
recipe_type=configure

configure_options=(
   --with-moddir="$goboModules"
   --mandir="${target}/man"
   --enable-zlib
)
build_variables=(
   "DOCBOOKTOMAN=''"
)

install_variables=(
   INSTALL=install
)

pre_build(){
   touch modprobe.conf.5
}
